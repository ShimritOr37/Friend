<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:facebook="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@color/black" >
    
  

    <com.facebook.widget.LoginButton
        android:id="@+id/authButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="30dp"
        android:onClick="buttonClicks2" />

   
    <com.facebook.widget.ProfilePictureView
        android:id="@+id/profilePicture"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        app:is_cropped="true"
        app:preset_size="large" />
    
   
     <ImageButton
         android:id="@+id/matchMyself"
         style="?android:attr/buttonStyleSmall"
         android:layout_width="wrap_content"
         android:layout_height="50dp"
         android:onClick="buttonClicks"
         android:scaleType="fitStart"
         android:src="@drawable/r42" />
    
    <ImageButton
        android:id="@+id/pickFriendsButton"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:onClick="onClick"
        android:scaleType="fitStart"
        android:src="@drawable/r43" />
     <ImageButton
        android:id="@+id/publishCom"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:background="@android:drawable/btn_default_small"
        android:onClick="buttonClicks"
        android:scaleType="fitStart"
        android:src="@drawable/r44" />
    
     <HorizontalScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp" >
         
         <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >
            
       <TextView
        android:id="@+id/con"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Today Best People to match-->>  "
        
        android:textColor="@color/LightPink" />
    
              
      <com.facebook.widget.ProfilePictureView
        android:id="@+id/sp1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        app:is_cropped="true"
        app:preset_size="small" />
                     
      <com.facebook.widget.ProfilePictureView
        android:id="@+id/sp2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        app:is_cropped="true"
        app:preset_size="small" />
                     
      <com.facebook.widget.ProfilePictureView
        android:id="@+id/sp3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        app:is_cropped="true"
        app:preset_size="small" />
      
       <com.facebook.widget.ProfilePictureView
        android:id="@+id/sp4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        app:is_cropped="true"
        app:preset_size="small" />
       
             <com.facebook.widget.ProfilePictureView
        android:id="@+id/sp5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        app:is_cropped="true"
        app:preset_size="small" />
                     
      <com.facebook.widget.ProfilePictureView
        android:id="@+id/sp6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        app:is_cropped="true"
        app:preset_size="small" />
                     
      <com.facebook.widget.ProfilePictureView
        android:id="@+id/sp7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        app:is_cropped="true"
        app:preset_size="small" />
      
       <com.facebook.widget.ProfilePictureView
        android:id="@+id/sp8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        app:is_cropped="true"
        app:preset_size="small" />
             <com.facebook.widget.ProfilePictureView
        android:id="@+id/sp8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        app:is_cropped="true"
        app:preset_size="small" />
                     
      <com.facebook.widget.ProfilePictureView
        android:id="@+id/sp9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        app:is_cropped="true"
        app:preset_size="small" />
                     
      <com.facebook.widget.ProfilePictureView
        android:id="@+id/sp10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        app:is_cropped="true"
        app:preset_size="small" />
      
       <com.facebook.widget.ProfilePictureView
        android:id="@+id/sp11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        app:is_cropped="true"
        app:preset_size="small" />
       
        </LinearLayout>
     </HorizontalScrollView>
       
        

    <TextView
        android:id="@+id/resultsTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="" />
    
     <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >
            
  
    <TextView
        android:id="@+id/namef"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/LightPink" />

    <TextView
        android:id="@+id/birthday"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/LightPink"
        android:visibility="visible" />

    <TextView
        android:id="@+id/relationship"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/LightPink" />
    

       </LinearLayout>
  
    

        

</LinearLayout>