<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.shimrit.pickforu"
          android:versionCode="3"
          android:versionName="1.0">
    <uses-sdk android:minSdkVersion="14"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    

    <application android:label="@string/app_name"
                 android:icon="@drawable/clogo"
                 android:theme="@android:style/Theme.NoTitleBar"
                 android:name="com.shimrit.pickforu.FriendPickerApplication"
            >
        <activity android:name="com.shimrit.pickforu.FriendPickerSampleActivity"
            android:theme="@android:style/Theme.Translucent"
                  android:label="@string/app_name"
                  android:windowSoftInputMode="adjustResize">
               <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.facebook.samples.sessionlogin.LoginUsingAtivityActivity">
             <intent-filter>
                <action android:name="com.someone.wants.to.call.me"></action>
            </intent-filter>
	</activity>
        <activity android:name="com.shimrit.pickforu.PickFriendsActivity"
                  android:label="Pick Friends">
        </activity>
        <activity android:name="com.shimrit.pickforu.MainFragment"
                  android:label="login">
        </activity>
          <activity android:name="com.shimrit.pickforu.DateChoser"
                  android:label="date pick">
        </activity>
         <activity android:name="com.shimrit.pickforu.DBforFriends"
                  android:label="db Friends">
        </activity>
        <activity android:name="com.shimrit.pickforu.Test"
                  android:label="Test">
        </activity>
        <activity android:name="com.shimrit.pickforu.MatchFriend"
                  android:label="Match Friends">
        </activity>
    
         <activity android:name="com.shimrit.pickforu.GraphApiSampleActivity"
                  android:label="Graph">
        </activity>
        <activity android:name="com.shimrit.pickforu.DragActivity"
                  android:label="Drag">
        </activity>
         <activity android:name="com.shimrit.pickforu.DragActivity$MyDragListener"
                  android:label="listener">
        </activity>
 
         <activity android:name="com.shimrit.pickforu.MyListActivity"
                  android:label="@string/app_name" >
          
        </activity>
        <activity android:name="com.facebook.LoginActivity"
                  android:theme="@android:style/Theme.Translucent.NoTitleBar"
                  android:label="@string/app_name" />
        <meta-data android:name="com.facebook.sdk.ApplicationId" android:value="@string/app_id"/>

    </application>
</manifest>
